    <select id="selectByFilter" parameterType="com.lmbx.file.web.file.filter.FileInfoFilter" resultMap="BaseResultMap">
    select
    ID, NAME, FILEID,FILESIZE/1024 as FILESIZE, PROJECT, TYPE, UPLOAD_TIME, CONTENTTYPE, FILETYPE
    from FILE_INFO where 1=1
    <if test="id != null and id != ''">
      AND id like CONCAT('%',#{id},'%')
    </if>
    <if test="type != null">
      AND type like CONCAT('%',#{type},'%')
    </if>
    <if test="project != null">
      AND project like CONCAT('%',#{project},'%')
    </if>

    <if test="filesizemax != null">
      AND filesize &lt; #{filesizemax}
    </if>
    <if test="filesizemin != null">
      AND filesize &gt; #{filesizemin}
    </if>


    <if test="fileId != null">
      AND fileid like CONCAT('%',#{fileId},'%')
    </if>
    <if test="name != null">
      AND name like CONCAT('%',#{name},'%')
    </if>

    <if test="startTime != null">
      and trunc(upload_Time, 'dd') &gt;= #{startTime}
    </if>
    <if test="endTime != null">
      and trunc(upload_Time, 'dd') &lt;= #{endTime}
    </if>
    
    <if test="filetype != null">
      AND filetype = #{filetype}
    </if>
    order by upload_Time DESC
    </select>
